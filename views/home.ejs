<%- include("partials/header"); -%>


<h1>Home</h1>
<p><%= homeStartingContent %></p>
 <% posts.forEach(function(post){ %>
    <h1><%=post.Title  %></h1>
    <form action="/posts/:<%=post.Title%>" method="get">
        <p><%= post.content %> <a href="/posts/<%=post.Title%>" class="href">Read more</a></p>
    </form> 
<%  }) %>

<%- include("partials/footer"); -%> 
